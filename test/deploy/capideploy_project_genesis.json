            "size": 1,
            "type": "gp1",
            "permissions": 777,
            "owner": "{ssh_user}",
            "id": ""
        }
    },
    "file_groups_up": {
        "up_all_cfg": {
            "src": "/tmp/capi_cfg",
            "dst": "/mnt/capi_cfg",
            "dir_permissions": 777,
            "file_permissions": 666,
            "owner": "{ssh_user}",
            "after": {
                "env": {
                    "LOCAL_CFG_LOCATION": "/mnt/capi_cfg",
                    "MOUNT_POINT_CFG": "/mnt/capi_cfg",
                    "MOUNT_POINT_IN": "/mnt/capi_in",
                    "MOUNT_POINT_OUT": "/mnt/capi_out"
                },
                "cmd": [
                    "sh/capiscripts/adjust_cfg_in_out.sh"
                ]
            }
        },
        "up_capiparquet_binary": {
            "src": "../../build/linux/amd64/capiparquet.gz",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 644,
            "after": {
                "env": {
                    "CAPI_BINARY": "/home/{ssh_user}/bin/capiparquet"
                },
                "cmd": [
                    "sh/capiscripts/unpack_capi_binary.sh"
                ]
            }
        },
        "up_daemon_binary": {
            "src": "../../build/linux/amd64/capidaemon.gz",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 644,
            "after": {
                "env": {
                    "CAPI_BINARY": "/home/{ssh_user}/bin/capidaemon"
                },
                "cmd": [
                    "sh/capiscripts/unpack_capi_binary.sh"
                ]
            }
        },
        "up_daemon_env_config": {
            "src": "../../pkg/exe/daemon/capidaemon.json",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 644,
            "after": {}
        },
        "up_diff_scripts": {
            "src": "./diff",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 744,
            "after": {}
        },
        "up_lookup_bigtest_in": {
