    "volumes": {
        "cfg": {
            "name": "{deployment_name}_cfg",
            "mount_point": "/mnt/capi_cfg",
            "size": 1,
            "type": "gp1",
            "permissions": 777,
            "owner": "{sftp_user}",
            "id": "e14d7ed0-ffd3-47d9-afd1-54a0411ba329"
        },
        "in": {
            "name": "{deployment_name}_in",
            "mount_point": "/mnt/capi_in",
            "size": 1,
            "type": "gp1",
            "permissions": 777,
            "owner": "{sftp_user}",
            "id": "6f7e276c-c406-4833-825d-3f4573989509"
        },
        "out": {
            "name": "{deployment_name}_out",
            "mount_point": "/mnt/capi_out",
            "size": 1,
            "type": "gp1",
            "permissions": 777,
            "owner": "{sftp_user}",
            "id": "e9a7fada-6def-40c3-8d5f-da8ff7295009"
        }
    },
    "file_groups_up": {
        "up_all_cfg": {
            "src": "/tmp/capi_cfg",
            "dst": "/mnt/capi_cfg",
            "dir_permissions": 777,
            "file_permissions": 666,
            "owner": "{sftp_user}",
            "after": {
                "env": {
                    "MOUNT_POINT_CFG": "sftp://{sftp_user}@{internal_bastion_ip}/mnt/capi_cfg",
                    "MOUNT_POINT_CFG_LOCAL": "/mnt/capi_cfg",
                    "MOUNT_POINT_IN": "sftp://{sftp_user}@{internal_bastion_ip}/mnt/capi_in",
                    "MOUNT_POINT_OUT": "sftp://{sftp_user}@{internal_bastion_ip}/mnt/capi_out"
                },
                "cmd": [
                    "sh/capiscripts/adjust_cfg_in_out.sh"
                ]
            }
        },
        "up_daemon_binary": {
            "src": "../../build/linux/amd64/capidaemon.gz",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 644,
            "after": {
                "env": {
                    "CAPI_BINARY": "/home/{ssh_user}/bin/capidaemon"
                },
                "cmd": [
                    "sh/capiscripts/unpack_capi_binary.sh"
                ]
            }
        },
        "up_daemon_env_config": {
            "src": "../../pkg/exe/daemon/capidaemon.json",
            "dst": "/home/{ssh_user}/bin",
            "dir_permissions": 744,
            "file_permissions": 644,
            "after": {}
        },
        "up_lookup_bigtest_in": {
