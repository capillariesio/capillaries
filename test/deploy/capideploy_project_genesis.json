        },
        "in": {
            "name": "{deployment_name}_in",
            "mount_point": "/mnt/capi_in",
            "size": 1,
            "type": "gp1",
            "permissions": 777,
            "owner": "{sftp_user}",
            "id": "fb002ef1-7d03-4307-9f5b-943a3e93bfe8"
