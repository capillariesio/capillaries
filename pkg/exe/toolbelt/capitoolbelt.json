{
    "handler_executable_type":"capi_toolbelt",
    "cassandra":{
      "hosts": ["127.0.0.1"],
      "port": 9042,
      "username": "cassandra",
      "password": "cassandra",
      "ssl_opts": {"ca_path":"~/.ssh/sf-class2-root.crt", "enable_host_verification":false},
      "keyspace_replication_config": "{ 'class' : 'NetworkTopologyStrategy', 'datacenter1' : 1 }",
      "consistency": "LOCAL_QUORUM",
      "disable_initial_host_lookup": false,
      "writer_workers": 20,
      "min_inserter_rate": 10,
      "num_conns": 2,
      "timeout": 5000,
      "connect_timeout": 1000
    },
    "amqp":{
      "url": "amqp://guest:guest@127.0.0.1/",
      "exchange": "capillaries",
      "prefetch_count": 20,
      "prefetch_size": 0
    },
    "thread_pool_size": 5,
    "custom_processors":{
      "py_calc":{
        "python_interpreter_path":"some_non_empty_python_path"
      },
      "tag_and_denormalize":{}
    },
    "log":{
          "level": "debug"
      }
  }