{
  "handler_executable_type": "capi_daemon",
  "cassandra": {
    "hosts": ["127.0.0.1"],
    "port": 9042,
    "username": "cassandra",
    "password": "cassandra",
    "keyspace_replication_config": "{ 'class' : 'NetworkTopologyStrategy', 'datacenter1' : 1 }",
    "writer_workers": 1,
    "num_conns": 2,
    "timeout": 5000,
    "connect_timeout": 1000
  },
  "amqp091": {
    "url": "amqp://guest:guest@127.0.0.1/",
    "exchange": "capillaries",
    "prefetch_count": 20,
    "prefetch_size": 0
  },
  "custom_processors": {
    "py_calc": {
      "python_interpreter_path": "python",
      "python_interpreter_params": ["-u", "-" ]
    },
    "tag_and_denormalize": {}
  },
  "daemon": {
    "thread_pool_size": 1,
    "dead_letter_ttl": 10000
  },
  "log": {
    "level": "debug",
    "prometheus_exporter_port": 9200
  }
}