        "in": {
            "name": "{deployment_name}_in",
            "mount_point": "/mnt/capi_in",
            "size": 1,
            "permissions": 644,
            "id": ""
        },
        "out": {
            "name": "{deployment_name}_out",
            "mount_point": "/mnt/capi_out",
            "size": 1,
            "permissions": 666,
            "id": ""
        }
    },
    "instances": {
        "bastion": {
            "host_name": "{deployment_name}-bastion",
            "ip_address": "10.5.0.2",
            "floating_ip_address": "{floating_ip_address}",
            "flavor": "gp1.semisonic",
            "image": "Ubuntu-22.04",
            "attached_volumes": {
