            },
            "id": "",
            "service": {
                "env": null,
                "priority": 10,
                "cmd": {
                    "setup": "service_setup_webapi_ui.sh",
                    "start": "service_start_webapi_ui.sh",
                    "stop": "service_stop_webapi_ui.sh"
                }
            },
            "applicable_file_groups": [
                "up_capi_scripts",
                "up_webapi_binaries",
                "up_ui",
                "down_lookup_results"
            ]
